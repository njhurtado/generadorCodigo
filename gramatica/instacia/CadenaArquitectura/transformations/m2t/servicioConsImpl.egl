[% import "../utils.eol"; 
var nombreClase=clase.nombreClase;
var nomPkgDao=Arq!Dao.all.get(0).paquete;

var nomPkgDaoFactory = nomPkgDao.substring(0,nomPkgDao.indexOf('.dao')+4);

%]
